{
  "collectionName": "components_product_image_galleries",
  "info": {
    "displayName": "Image Gallery",
    "description": "Product image gallery with multiple images"
  },
  "options": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 100
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": ["images"]
    },
    "layout": {
      "type": "enumeration",
      "enum": ["grid", "carousel", "masonry"],
      "default": "grid"
    }
  }
}